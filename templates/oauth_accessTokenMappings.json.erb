{
  "id": "authz_req|@@aid@@|<%= @oauth_svc_acc_tok_mgr_id -%>",
  "context": {
    "type": "IDP_ADAPTER", 
    "contextRef": {
      "id": "@@aid@@", 
      "location": "<%= @adm_api_baseURL -%>/idp/adapters/@@aid@@"
    }
  }, 
  "accessTokenManagerRef": {
    "id": "<%= @oauth_svc_acc_tok_mgr_id -%>", 
    "location": "<%= @adm_api_baseURL -%>/oauth/accessTokenManagers/<%= @oauth_svc_acc_tok_mgr_id -%>"
  }, 
  "attributeContractFulfillment": {
<% @b['oauth_token_map'].each_with_index do |val, i| -%>
      "<%= val['name'] -%>": {
        "source": {
          "type": "<%= val['type'] -%>"
        },
        "value": "<%= val['value'] -%>"
      }<%= ',' if i < (@b['oauth_token_map'].size - 1) %>
<% end -%>
  }
}
